  </eClassifiers>
  <eClassifiers xsi:type="ecore:EEnum" name="ButtonPlacementType">
    <eLiterals name="RIGHT_OF_LABEL"/>
    <eLiterals name="LEFT_OF_LABEL" value="1"/>
  </eClassifiers>
</ecore:EPackage>
