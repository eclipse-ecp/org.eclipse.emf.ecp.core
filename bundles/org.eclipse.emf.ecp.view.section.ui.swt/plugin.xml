<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.emf.ecp.ui.view.swt.renderers">
      <renderer
            renderer="org.eclipse.emf.ecp.view.spi.section.swt.SectionLeafSWTRenderer">
         <dynamicTest
               testClass="org.eclipse.emf.ecp.view.internal.section.swt.LeafRendererTester">
         </dynamicTest>
      </renderer>
      <renderer
            renderer="org.eclipse.emf.ecp.view.spi.section.swt.SectionNodeSWTRenderer">
         <staticTest
               element="org.eclipse.emf.ecp.view.spi.section.model.VSection"
               priority="5">
         </staticTest>
      </renderer>
      <renderer
            renderer="org.eclipse.emf.ecp.view.spi.section.swt.SectionedAreaSWTRenderer">
         <staticTest
               element="org.eclipse.emf.ecp.view.spi.section.model.VSectionedArea"
               priority="5">
         </staticTest>
      </renderer>
   </extension>

</plugin>
