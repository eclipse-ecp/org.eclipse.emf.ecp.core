get-editor View | get-editbox -after [get-label "Name*"] | set-text PlayerView
get-editor PlayerView | get-button "Link Root EClas" | click
get-window "Select an EClass." | get-tree | select "bowling/Player \\[org.eclipse.emf.emfstore.bowling.Player\\]" 
    | double-click
get-view "Model Explorer" | get-tree | select "Project/PlayerView" | get-menu Control | click
get-editor Control | get-editbox -after [get-label "Name*"] | set-text DateOfBirth
get-editor DateOfBirth | get-button "The Readonly of the Element" | check
get-view "Model Explorer" | get-tree | select "Project/PlayerView/DateOfBirth" 
    | get-menu "Feature Path Domain Model Reference" | click
get-editor "Feature Path Domain Model Reference" | get-button "Link Domain Model EFeature" | click
get-window "Select a EStructuralFeature" | get-tree | select "dateOfBirth : EDate" | double-click
with [get-view "Model Explorer"] {
    get-tree | select "Project/PlayerView"
    try { get-button "Save Project" | click} - catch { wait 500 | get-button "Save Project" | click }
}
get-editor "Player John" | close
get-editor PlayerView | click
get-view "Model Explorer" | get-tree | select "TestProject/Player John" | double-click
with [get-editor "Player John"] {
    get-date-time | get-property enablement | equals false | verify-true
    get-date-time -after [get-label "Date Of Birth"] -index 1 | get-property enablement | equals false | verify-true
}

get-view "Model Explorer" | get-tree | select "Project/PlayerView"
get-editor PlayerView | get-button "Delete Reference" | click
with [get-view "Model Explorer"] {
    get-tree | select Project
    try { get-button "Save Project" | click} - catch { wait 500 | get-button "Save Project" | click }
}
