Content-Type: text/ecl
Entry-Name: .content

try {
	get-editor View | get-button "Link Root EClass" | click
	with [get-window "Select an EClass."] {
		get-tree | select "bowling/League \\[org.eclipse.emf.emfstore.bowling" +
				".League\\]"
		get-button OK | click
	}

	with [get-view "Model Explorer" | get-tree] {
		select "Project/View" | get-menu TableControl | click
	}
	get-editor TableControl | get-button "Create and link new Domain Model Re" +
			"ference" | click

	with [get-window "New Reference Element"] {
		get-button "Link Domain Model EFeature" | click
		with [get-window "Select a EStructuralFeature"] {
			get-tree | select "players : Player"
			get-button OK | click
		}
		get-button Add | click
		with [get-window "New Reference Element"] {
			get-tree | select "model/FeaturePathDomainModelReference"
			get-button "Next >" | click
			get-button "Link Domain Model EFeature" | click
			with [get-window "Select a EStructuralFeature"] {
				get-tree | select "name : EString"
				get-button OK | click
			}
			get-button Finish | click
		}
	}
	with [get-window "New Reference Element" | get-table] {
		get-property itemCount | equals 1 | verify-true
		get-property "getItems().TableItem[0].toString()" | equals "TableItem" +
				" {name}" | verify-true
	}
	with [get-window "New Reference Element"] {
		get-button Finish | click
	}
} -finally {
	/* get-view "Model Explorer" | get-tree | select "Project/View" | double-click
	get-editor View | get-button "Delete Reference" | click
	with [get-view "Model Explorer"] {
		try {
		get-tree | select Project
		get-button "Save Project" | click
		} -catch {
			wait 1000
			try {
			get-tree | select "Project/View"
			get-button "Save Project" | click
			} -catch {
				wait 1000
				get-tree | select Project
				get-button "Save Project" | click
			}
		}
	} */
	get-view "Model Explorer" | get-tree | select "Project/View" | get-menu
			"Unset Root EClass" | click
}
