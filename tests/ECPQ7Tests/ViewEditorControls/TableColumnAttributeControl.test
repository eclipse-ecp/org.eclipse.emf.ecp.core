try {
get-editor View | get-button "Link Root EClass" | click
with [get-window "Select an EClass."] {
    get-tree | select "bowling/League \\[org.eclipse.emf.emfstore.bowling.League\\]"
    get-button OK | click
}
with [get-view "Model Explorer" | get-tree] {
    select "Project/View" | get-menu TableControl | click
    select "Project/View/TableControl" | get-menu "Table Domain Model Reference" | click
}
get-editor "Table Domain Model Reference" | get-button "Link Domain Model EFeature" | click
with [get-window "Select a EStructuralFeature"] {
    get-tree | select "players : Player"
    get-button OK | click
}
get-view "Model Explorer" | get-tree | select "Project/View/TableControl" | get-menu Column | click
with [get-editor "Column false"] {
    get-label "Attribute*" | get-property caption | equals "Attribute*" | verify-true
    get-button "Link Attribute" | get-property tooltip | equals "Link Attribute" | verify-true
}
get-editor "Column false" | get-button "Link Attribute" | click
get-window "Select Attribute for TableColumn" | get-table | get-property itemCount | equals 9 | verify-true
with [get-window "Select Attribute for TableColumn"] {
    get-table | select "name : EString"
    get-button OK | click
}
get-editor "Column false" | get-link "<a>name : EString</a>" | get-property caption | equals "<a>name : EString</a>" 
    | verify-true
   
} -finally { 
