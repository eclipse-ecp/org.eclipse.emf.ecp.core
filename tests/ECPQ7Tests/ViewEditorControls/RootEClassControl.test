with [get-editor View] {
    get-label "Root EClass*" | get-property caption | equals "Root EClass*" | verify-true
    get-button "Link Root EClas" | get-property tooltip | equals "Link Root EClas" | verify-true
}
get-editor View | get-button "Link Root EClas" | click
get-window "Select an EClass." | get-tree | select accesscontrol
with [get-window "Select an EClass."] {
    get-label "Select an EClass." | get-property caption | equals "Select an EClass." | verify-true
    get-label "This is not an EClass." | get-property caption | equals "This is not an EClass." | verify-true
}
get-window "Select an EClass." | get-tree | select "bowling/Player \\[org.eclipse.emf.emfstore.bowling.Player\\]"
with [get-window "Select an EClass."] {
    get-label OK | get-property caption | equals OK | verify-true
    get-button OK | get-property caption | equals OK | verify-true
}
get-window "Select an EClass." | get-button OK | click
get-editor View | get-link "<a>Player [org.eclipse.emf.emfstore.bowling.Player]</a>" | get-property caption 
    | equals "<a>Player [org.eclipse.emf.emfstore.bowling.Player]</a>" | verify-true
    
get-view "Model Explorer" | get-tree | select "Project/View"
get-editor View | get-button "Delete Reference" | click
with [get-view "Model Explorer"] {
    get-tree | select Project
    try { get-button "Save Project" | click} - catch { wait 500 | get-button "Save Project" | click }
}
