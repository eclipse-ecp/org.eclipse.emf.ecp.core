get-editor View | get-button "Link Root EClas" | click
with [get-window "Select an EClass."] {
    get-tree | select "bowling/Player \\[org.eclipse.emf.emfstore.bowling.Player\\]"
    get-button OK | click
}
with [get-view "Model Explorer" | get-tree] {
    select "Project/View" | get-menu Control | click
    select "Project/View/Control" | get-menu "Feature Path Domain Model Reference" | click
}
get-editor "Feature Path Domain Model Reference" | get-button "Link Domain Model EFeature" | click
with [get-window "Select a EStructuralFeature"] {
    get-tree | select "gender : Gender"
    get-button OK | click
}
with [get-view "Model Explorer" | get-tree] {
    select "Project/View/Control" | get-menu "Show Rule" | click
    select "Project/View/Control/Show Rule false" | get-menu "Leaf Condition" | click
    select "Project/View/Control/Show Rule false/Leaf Condition" | get-menu "Feature Path Domain Model Reference" 
        | click
}
with [get-editor "Feature Path Domain Model Reference" -index 1] {
    get-label "Domain Model EFeature*" | get-property caption | equals "Domain Model EFeature*" | verify-true
    get-button "Link Domain Model EFeature" | get-property tooltip | equals "Link Domain Model EFeature" | verify-true
}
get-editor "Feature Path Domain Model Reference" -index 1 | get-button "Link Domain Model EFeature" | click
get-window "Select a EStructuralFeature" | get-tree | select "name : EString"
with [get-window "Select a EStructuralFeature"] {
    get-label "Select a EStructuralFeature" | get-property caption | equals "Select a EStructuralFeature" | verify-true
    get-label OK | get-property caption | equals OK | verify-true
    get-tree | get-property itemCount | equals 9 | verify-true
}
get-window "Select a EStructuralFeature" | get-button OK | click
get-editor "Feature Path Domain Model Reference" -index 1 | get-link "<a>name : EString</a>" | get-property caption 
    | equals "<a>name : EString</a>" | verify-true
    
get-view "Model Explorer" | get-tree | select "Project/View"
get-editor View | get-button "Delete Reference" | click
with [get-view "Model Explorer"] {
    get-tree | select Project
    try { try { get-button "Save Project" | click} - catch { wait 500 | get-button "Save Project" | click }} -catch{try { get-button "Save Project" | click} - catch { wait 500 | get-button "Save Project" | click }}
}
