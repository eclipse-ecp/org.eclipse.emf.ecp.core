Entry-Name: .content

get-view "Model Explorer" | get-tree | select "TestProject/League" | double-click
get-editor League | get-editbox -after [get-label Name] | set-text Test
get-view "Model Explorer" | get-tree | select "TestProject/League Test" | get-menu Player | click
get-editor Player | get-editbox -after [get-label Name] | set-text Jonas
with [get-editor "Player Jonas"] {
    get-editbox -after [get-label Height] | set-text "10"
    get-button -after [get-label "Is Professional"] | click
    get-button "Add Entry" | click
    with [get-editbox -after [get-label "EMails.*"]] {
        set-text jonas
        key-type "M1+M3+q" "@"
        set-text "jonas@example.org"
    }
}
get-view "Model Explorer" | get-tree | select "TestProject/League Test" | get-menu Player | click
with [get-editor Player | get-editbox -after [get-label Name]] {
    set-text Max
    key-type "TRAVERSE_TAB_NEXT"
}
with [get-editor "Player Max"] {
    get-button "Add Entry" | click
    with [get-editbox -after [get-label "EMails.*"]] {
        set-text max
        key-type "M1+M3+q" "@"
        set-text "max@example.org"
    }
    get-editbox -after [get-label "Number Of Victories"] | set-text 1
    get-editbox -after [get-label Height] | set-text "20"
    get-button -after [get-label "No date set! Click to set date."] | click
    get-window -from "DateTimeControlSWTRenderer$SetBtnSelectionAdapterExtension.widgetSelected()" | get-date-time 
    | set-value "2014/5/1 0:0:0"
    get-date-time | set-value "1980/4/10 16:48:17"
    get-editbox -after [get-label Name] | click-text 1
}
