    get-property "getChildren().Control[2].getChildren().Control[0].getChildren().Control[0].getChildren().length" 
        | equals 3 | verify-true
    get-property "getChildren().Control[2].getChildren().Control[0].getChildren().Control[0].getChildren().Control[0].getClassName()" 
        | equals "org.eclipse.swt.widgets.DateTime" | verify-true
    get-property "getChildren().Control[2].getChildren().Control[0].getChildren().Control[0].getChildren().Control[0].getStyle()" 
        | equals "[LEFT_TO_RIGHT, DATE, MEDIUM]" | verify-true
