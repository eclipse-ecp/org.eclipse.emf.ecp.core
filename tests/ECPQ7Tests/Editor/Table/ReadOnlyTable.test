with [get-editor View | get-editbox -after [get-label "Name*"]] {
    set-text TableTest
}
with [get-view "Model Explorer" | get-tree] {
    select "Project/TableTest" | get-menu Control | click
    select "Project/TableTest"
}
get-editor TableTest | get-button "Link Root EClas" | click
with [get-window "Select an EClass."] {
    get-tree | select "bowling/Matchup \\[org.eclipse.emf.emfstore.bowling.Matchup\\]"
    get-button OK | click
}
get-view "Model Explorer" | get-tree | select "Project/TableTest/Control"
with [get-editor Control | get-editbox -after [get-label "Name*"]] {
    set-text matchupView
}
get-view "Model Explorer" | get-tree | select "Project/TableTest/matchupView" 
    | get-menu "Feature Path Domain Model Reference" | click
get-editor "Feature Path Domain Model Reference" | get-button "Link Domain Model EFeature" | click
with [get-window "Select a EStructuralFeature"] {
    get-tree | select "games : Game"
    get-button OK | click
}
get-view "Model Explorer" | get-tree | select "Project/TableTest/matchupView"
with [get-view "Model Explorer" | get-tree] {
    select "Project/TableTest/matchupView" | double-click
    select "TestProject/Tournament Amateur/Matchup" | double-click
    select "Project/TableTest/matchupView"
}
get-editor matchupView | get-button "The Readonly of the Element" | check
with [get-view "Model Explorer"] {
    get-tree | select "Project/TableTest"
   try { try { get-button "Save Project" | click} - catch { wait 500 | get-button "Save Project" | click }} -catch{try { get-button "Save Project" | click} - catch { wait 500 | get-button "Save Project" | click }}
}

get-editor Matchup | close
get-view "Model Explorer" | get-tree | select "TestProject/Tournament Amateur/Matchup" | double-click
with [get-editor Matchup | get-control Any -after [get-label Games] -index 1] {
    get-property "getChildren().Control[0].isVisible()" | equals false | verify-true
    get-property "getChildren().Control[1].isVisible()" | equals false | verify-true
}
get-view "Model Explorer" | get-tree | select "Project/TableTest"
get-editor TableTest | get-button "Delete Reference" | click
with [get-view "Model Explorer"] {
    get-tree | select Project
    try { get-button "Save Project" | click} - catch { wait 500 | get-button "Save Project" | click }
}
