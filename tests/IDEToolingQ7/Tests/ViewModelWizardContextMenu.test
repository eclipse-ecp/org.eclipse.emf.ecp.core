Content-Type: text/ecl
Entry-Name: .content

get-view "Package Explorer" | get-tree | select "com.eclipsesource.makeithappen.model/model/task.ecore" 
    | get-menu "EMF Forms/Create View Model" | click
get-window -class WizardDialog | get-tree | select "task/Task" | key-type "M1+M3+7" "{"
with [get-window -class WizardDialog] {
    get-label "Select EClass" | get-property caption | equals "Select EClass" | verify-true
    get-editbox -after [get-label "Select EClass"] | get-property text 
        | equals "Select an EClass for the current View Model." | verify-true
    get-label "Select an EClass:" | get-property caption | equals "Select an EClass:" | verify-true
    with [get-tree] {
        get-item task | get-property caption | equals task | verify-true
        get-item task | get-property childCount | equals 3 | verify-true
        get-item "task/Task" | get-property caption | equals Task | verify-true
        get-item "task/User" | get-property caption | equals User | verify-true
        get-item "task/UserGroup" | get-property caption | equals UserGroup | verify-true
    }
    with [get-button "Fill view model with default layout"] {
        get-property enablement | equals true | verify-true
        get-property caption | equals "Fill view model with default layout" | verify-true
        get-property kind | equals Check | verify-true
    }
}

with [get-window -class WizardDialog] {
    get-button "Fill view model with default layout" | get-property selected | equals true | verify-true
    get-button "Next >" | get-property enablement | equals true | verify-true
    get-button Finish | get-property enablement | equals true | verify-true
    get-button "< Back" | get-property enablement | equals true | verify-true
}
get-window -class WizardDialog | get-button "Next >" | click
with [get-window -class WizardDialog] {
    get-editbox -after [get-label "File name:"] | get-property text | equals "Task.view" | verify-true
    get-button Finish | get-property enablement | equals true | verify-true
    get-button "Next >" | get-property enablement | equals false | verify-true
}
get-window -class WizardDialog | get-button Finish | click
with [get-tab-folder -index 1] {
    get-property activePage | equals "Task.view" | verify-true
    get-property tabCount | equals 1 | verify-true
}
with [get-editor "Task.view" | get-tree] {
    get-item View | get-property caption | equals View | verify-true
    get-item View | get-property childCount | equals 6 | verify-true
}
get-view "Package Explorer" | get-tree | select "com.eclipsesource.makeithappen.model/model/task.ecore" 
    | get-menu "EMF Forms/Create View Model" | click
with [get-window -class WizardDialog] {
    get-tree | select "task/User"
    get-button "Next >" | click
}
with [get-window -class WizardDialog] {
    get-editbox -after [get-label "File name:"] | get-property text | equals "User.view" | verify-true
    get-button Finish | get-property enablement | equals true | verify-true
}
get-window -class WizardDialog | get-button Finish | click
with [get-tab-folder -index 1] {
    get-property "pages[0]" | equals "Task.view" | verify-true
    get-property "pages[1]" | equals "User.view" | verify-true
    get-property activePage | equals "User.view" | verify-true
    get-property tabCount | equals 2 | verify-true
}
with [get-editor "User.view"] {
    with [get-tree] {
        get-item View | get-property caption | equals View | verify-true
        get-item View | get-property childCount | equals 11 | verify-true
    }
    get-section Details | get-label User | get-property caption | equals User | verify-true
}
get-view "Package Explorer" | get-tree | select "com.eclipsesource.makeithappen.model/model/task.ecore" 
    | get-menu "EMF Forms/Create View Model" | click
with [get-window -class WizardDialog] {
    get-tree | select "task/UserGroup"
    get-button "Fill view model with default layout" | uncheck
    get-button "Next >" | click
    get-button Finish | click
}
with [get-tab-folder -index 1] {
    get-property "pages[0]" | equals "Task.view" | verify-true
    get-property "pages[1]" | equals "User.view" | verify-true
    get-property "pages[2]" | equals "UserGroup.view" | verify-true
    get-property activePage | equals "UserGroup.view" | verify-true
    get-property tabCount | equals 3 | verify-true
}
with [get-editor "UserGroup.view"] {
    with [get-tree] {
        get-item View | get-property caption | equals View | verify-true
        get-item View | get-property childCount | equals 0 | verify-true
    }
    get-section Details | get-label UserGroup | get-property caption | equals UserGroup | verify-true
}
