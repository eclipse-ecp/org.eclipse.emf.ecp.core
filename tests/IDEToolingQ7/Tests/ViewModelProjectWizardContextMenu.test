Content-Type: text/ecl
Entry-Name: .content

get-view "Package Explorer" | get-tree | select "com.eclipsesource.makeithappen.model/model/task.ecore" 
    | get-menu "EMF Forms/Create View Model Project" | click
get-window "New View Model Project" | get-editbox -after [get-label "Project name:"] | get-property text 
    | equals "com.eclipsesource.makeithappen.model.viewmodel" | verify-true
with [get-window "New View Model Project"] {
    get-button "Next >" | click
    get-tree | select "task/User"
}
get-window "New View Model Project" | get-button Finish | click
get-view "Package Explorer" | get-tree | select "com.eclipsesource.makeithappen.model.viewmodel/viewmodels/User.view"
with [get-tab-folder -index 1] {
    get-property "pages[0]" | equals "User.view" | verify-true
    get-property activePage | equals "User.view" | verify-true
}
with [get-editor "User.view" | get-tree] {
    get-item View | get-property caption | equals View | verify-true
    get-item View | get-property childCount | equals 11 | verify-true
}
get-view "Package Explorer" | get-tree | select "com.eclipsesource.makeithappen.model.viewmodel/plugin.xml" 
    | double-click
get-editor "com.eclipsesource.makeithappen.model.viewmodel" | get-tab-folder | get-tab-item "plugin.xml" | click
get-editor "com.eclipsesource.makeithappen.model.viewmodel" | get-text-viewer | get-property text 
    | equals "<?xml version=\"1.0\" encoding=\"UTF-8\"?>\n"
    + "<?eclipse version=\"3.4\"?>\n"
    + "<plugin>\n"
    + "   <extension\n"
    + "         point=\"org.eclipse.emf.ecp.view.model.provider.xmi.file\">\n"
    + "      <file\n"
    + "            filePath=\"viewmodels/User.view\">\n"
    + "      </file>\n"
    + "   </extension>\n\n"
    + "</plugin>" | verify-true
get-editor "com.eclipsesource.makeithappen.model.viewmodel" | get-tab-folder | get-tab-item "MANIFEST.MF" | click
get-editor "com.eclipsesource.makeithappen.model.viewmodel" | get-text-viewer | get-property text 
    | equals "Manifest-Version: 1.0\n"
    + "Bundle-ManifestVersion: 2\n"
    + "Bundle-Name: com.eclipsesource.makeithappen.model.viewmodel\n"
    + "Bundle-SymbolicName: com.eclipsesource.makeithappen.model.viewmodel;singleton:=true\n"
    + "Bundle-Version: 1.0.0.qualifier\n"
    + "Require-Bundle: org.eclipse.emf.ecp.view.model.provider.xmi" | verify-true
