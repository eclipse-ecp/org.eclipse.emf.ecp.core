Content-Type: text/ecl
Entry-Name: .content



get-view "Package Explorer" | get-tree | get-menu "New/Other..." | click
with [get-window New] {
    get-tree | select "EMF Forms/View Model Project"
    get-button "Next >" | click
}
get-window "New View Model Project" | get-editbox -after [get-label Project] | get-property text 
    | equals "Create a new View Model Project." | verify-true
with [get-window "New View Model Project"] {
    get-editbox -after [get-label "Project name:"] | set-text TestViewModelProject
    get-button "Next >" | click
    get-button "< Back" | click
    get-button "Next >" | click
    get-button Select | click
    with [get-window "Select Model"] {
        get-tree | select "com.eclipsesource.makeithappen.model/model/task.ecore"
        get-button OK | click
    }
}
with [get-window "New View Model Project"] {
    get-label "Selected Ecore:" | get-property caption | equals "Selected Ecore:" | verify-true
    get-editbox -after [get-label "Selected Ecore:"] | get-property text 
        | equals "/com.eclipsesource.makeithappen.model/model/task.ecore" | verify-true
}
with [get-window "New View Model Project"] {
    get-button "Next >" | get-property enablement | equals true | verify-true
    get-button Finish | get-property enablement | equals false | verify-true
}
get-window "New View Model Project" | get-button "Next >" | click
with [get-window "New View Model Project"] {
    get-button "Next >" | get-property enablement | equals false | verify-true
    get-button Finish | get-property enablement | equals false | verify-true
    with [get-tree] {
        get-item task | get-property caption | equals task | verify-true
        get-item task | get-property selection | equals false | verify-true
        get-item task | get-property childCount | equals 3 | verify-true
    }
    with [get-button "Fill view model with default layout"] {
        get-property enablement | equals true | verify-true
        get-property selected | equals true | verify-true
    }
}
get-window "New View Model Project" | get-tree | select "task/Task"
get-window "New View Model Project" | get-button Finish | get-property enablement | equals true | verify-true
get-window "New View Model Project" | get-button Finish | click
with [get-tab-folder -index 1] {
    get-property "pages[0]" | equals "Task.view" | verify-true
    get-property activePage | equals "Task.view" | verify-true
}
get-view "Package Explorer" | get-tree | get-item TestViewModelProject | get-property caption 
    | equals TestViewModelProject | verify-true
get-view "Package Explorer" | get-tree | get-item "TestViewModelProject/viewmodels/Task.view" | get-property caption 
    | equals "Task.view" | verify-true
get-view "Package Explorer" | get-tree | select "TestViewModelProject/plugin.xml" | double-click
with [get-editor TestViewModelProject | get-section "General Information"] {
    get-editbox -after [get-label "ID:"] | get-property text | equals TestViewModelProject | verify-true
    get-editbox -after [get-label "Name:"] | get-property text | equals TestViewModelProject | verify-true
}
get-editor TestViewModelProject | get-tab-folder | get-tab-item Dependencies | click
get-editor TestViewModelProject | get-section "Required Plug-ins" | get-table 
    | get-item "org.eclipse.emf.ecp.view.model.provider.xmi" | get-property caption 
    | equals "org.eclipse.emf.ecp.view.model.provider.xmi" | verify-true
with [get-editor TestViewModelProject] {
    with [get-tab-folder] {
        get-tab-item Runtime | click
        get-tab-item Extensions | click
    }
    get-section "All Extensions" | get-tree 
        | select "org.eclipse.emf.ecp.view.model.provider.xmi.file/viewmodels\\/Task.view \\(file\\)"
}
with [get-editor TestViewModelProject] {
    with [get-section "All Extensions" | get-tree] {
        get-item "org.eclipse.emf.ecp.view.model.provider.xmi.file" | get-property caption 
            | equals "org.eclipse.emf.ecp.view.model.provider.xmi.file" | verify-true
        get-item "org.eclipse.emf.ecp.view.model.provider.xmi.file/viewmodels\\/Task.view \\(file\\)" 
            | get-property caption | equals "viewmodels/Task.view (file)" | verify-true
    }
    get-section "Extension Element Details" | get-editbox -after [get-label "filePath:"] | get-property text 
        | equals "viewmodels/Task.view" | verify-true
}

