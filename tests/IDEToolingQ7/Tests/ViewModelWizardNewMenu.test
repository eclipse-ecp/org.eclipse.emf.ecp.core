Content-Type: text/ecl
Entry-Name: .content

get-view "Package Explorer" | get-tree | get-menu "New/Other..." | click
with [get-window New] {
    get-tree | select "EMF Forms/View Model"
    get-button "Next >" | click
    get-button Select | click
    with [get-window "Select Model"] {
        get-tree | select "com.eclipsesource.makeithappen.model/model/task.ecore"
        get-button OK | click
    }
    get-button "Next >" | click
    get-tree | select "task/Task"
    get-button "Next >" | click
    get-tree | select "com.eclipsesource.makeithappen.model/model"
    get-button "< Back" | click
    get-button "Next >" | click
    with [get-editbox -after [get-label "File name:"]] {
        click-text 4 3
        key-type Left
        key-type Right
        key-type "M2+ARROW_LEFT" -times 3
        set-text "Task.view"
    }
    get-button Finish | click
}
with [get-tab-folder -index 1] {
    get-property "pages[0]" | equals "Task.view" | verify-true
    get-property activePage | equals "Task.view" | verify-true
    get-property tabCount | equals 1 | verify-true
}
with [get-editor "Task.view" | get-tree] {
    get-item View | get-property caption | equals View | verify-true
    get-item View | get-property childCount | equals 6 | verify-true
}
get-view "Package Explorer" | get-tree | select "com.eclipsesource.makeithappen.model/model/task.ecore" 
    | get-menu "EMF Forms/Create View Model" | click
with [get-window -class WizardDialog] {
    get-tree | select "task/User"
    get-button "Fill view model with default layout" | uncheck
    get-button Finish | click
}
with [get-tab-folder -index 1] {
    get-property "pages[0]" | equals "Task.view" | verify-true
    get-property "pages[1]" | equals "My.view" | verify-true
    get-property activePage | equals "My.view" | verify-true
}
get-editor "My.view" | get-tree | get-item View | get-property childCount | equals 0 | verify-true
get-view "Package Explorer" | get-tree | select "com.eclipsesource.makeithappen.model/model/task.ecore" 
    | get-menu "EMF Forms/Create View Model" | click
with [get-window -class WizardDialog] {
    get-button "Next >" | get-property enablement | equals false | verify-true
    get-button Finish | get-property enablement | equals false | verify-true
    with [get-tree] {
        get-item task | get-property selection | equals false | verify-true
        get-item task | get-property selected | equals false | verify-true
    }
}
get-window -class WizardDialog | get-tree | select "task/Task"
with [get-window -class WizardDialog] {
    get-button "Next >" | get-property enablement | equals true | verify-true
    with [get-button "Fill view model with default layout"] {
        get-property enablement | equals true | verify-true
        get-property caption | equals "Fill view model with default layout" | verify-true
        get-property selected | equals true | verify-true
    }
}
get-window -class WizardDialog | get-button "Next >" | click
with [get-window -class WizardDialog] {
    get-editbox -after [get-label "File name:"] | get-property text | equals "Task1.view" | verify-true
    get-button Finish | get-property enablement | equals true | verify-true
}
get-window -class WizardDialog | get-button Finish | click
with [get-tab-folder -index 1] {
    get-property "pages[0]" | equals "Task.view" | verify-true
    get-property "pages[1]" | equals "My.view" | verify-true
    get-property "pages[2]" | equals "Task1.view" | verify-true
    get-property activePage | equals "Task1.view" | verify-true
}
with [get-editor "Task1.view"] {
    get-tree | get-item View | get-property childCount | equals 6 | verify-true
    get-section Details | get-label Task | get-property caption | equals Task | verify-true
}
