Content-Type: text/ecl
Entry-Name: .content

get-view "Package Explorer" | get-tree | select "com.eclipsesource.makeithappen.model.viewmodel/viewmodels/User.view" 
    | double-click
get-editor "User.view" | get-button "Open Preview" | click

get-view "Task List" | close
get-view Outline | close
with [get-editor "User.view" | get-tree] {
    select "View/Control firstName" "View/Control lastName" "View/Control gender" "View/Control active" "View/Control timeOfRegistration" "View/Control weight" "View/Control heigth" "View/Control nationality" "View/Control dateOfBirth" "View/Control eMails" "View/Control tasks"
    get-menu Delete | click
}
get-view "EMF Forms Preview" | get-button "Automatic Update" | click
get-view "EMF Forms Preview" | get-control Any -after [get-label "View Model Preview"] -index 3 
    | get-property "getChildren().length" | equals 0 | verify-true

with [get-editor "User.view"] {
    with [get-tree] {
        select View | get-menu Control | click
        select "View/Control"
    }
    with [get-section Details | get-editbox -after [get-label Name]] {
        set-text "Control c1"
        key-type "NUMPAD_ENTER" "\\u000D"
    }
    with [get-tree] {
        select "View/Control c1" | get-menu "Feature Path Domain Model Reference" | click
        select "View/Control c1/Feature Path Domain Model Reference"
    }
    get-section Details | get-button "Link Domain Model EFeature" | click
}
with [get-window "Select a EStructuralFeature"] {
    get-tree | select "firstName : EString"
    get-button OK | click
}
with [get-editor "User.view"] {
    with [get-tree] {
        select View | get-menu Control | click
        select "View/Control"
    }
    get-section Details | get-editbox -after [get-label Name] | set-text "Control c2"
    with [get-tree] {
        select "View/Control c2" | get-menu "Domain Model Reference" | click
        select "View/Control c2/Domain Model Reference"
    }
    with [get-section Details] {
        get-editbox -after [get-label "Bundle Name*"] | set-text bbb
        get-editbox -after [get-label "Class Name*"] | set-text bbb
    }
    with [get-tree] {
        select View | get-menu Control | click
        select "View/Control" | get-menu "Table Domain Model Reference" | click
        select "View/Control/Table Domain Model Reference"
    }
    get-section Details | get-button "Link Domain Model EFeature" | click
}
with [get-window "Select a EStructuralFeature"] {
    get-tree | select "eMails : EString"
    get-button OK | click
}
get-editor "User.view" | get-tree | select "View/Control/Table Domain Model Reference"


get-editor "User.view" | get-tree | select View | get-menu Categorization | click
get-view "EMF Forms Preview" | get-button "Automatic Update" | click
with [get-editor "User.view" | get-tree] {
    select "View/Categorization" | get-menu "Leaf Category" | click
    select "View/Categorization/Leaf Category" | get-menu VerticalLayout | click
    select "View/Categorization/Leaf Category/VerticalLayout" | get-menu "Generate Controls" | click
}
with [get-window -class SelectAttributesDialog] {
    with [get-table] {
        get-item "firstName : EString" | check
        get-item "firstName : EString" | uncheck
        get-item "firstName : EString" | check
        get-item "lastName : EString" | check
    }
    get-button OK | click
}
with [get-editor "User.view" | get-tree] {
    select "View/Categorization" | get-menu "Leaf Category" | click
    select [get-item "View/Categorization" | get-item "Leaf Category" -index 1] | get-menu VerticalLayout | click
    select [get-item "View/Categorization" | get-item "Leaf Category" -index 1 | get-item VerticalLayout] 
        | get-menu "Generate Controls" | click
}
with [get-window -class SelectAttributesDialog] {
    with [get-table] {
        get-item "active : EBoolean" | check
        get-item "timeOfRegistration : EDate" | check
        get-item "weight : EDouble" | check
        get-item "heigth : EInt" | check
    }
    get-button OK | click
}

get-view "EMF Forms Preview" | get-tree | select "Leaf Category"
with [get-view "EMF Forms Preview"] {
    get-label "Last Name*" | get-property caption | equals "Last Name*" | verify-true
    get-label "First Name" | get-property caption | equals "First Name" | verify-true
}
get-view "EMF Forms Preview" | get-tree | select [get-item "Leaf Category" -index 1]
with [get-view "EMF Forms Preview"] {
    get-label Heigth | get-property caption | equals Heigth | verify-true
    get-label "Time Of Registration" | get-property caption | equals "Time Of Registration" | verify-true
    get-label Weight | get-property caption | equals Weight | verify-true
    get-label Active | get-property caption | equals Active | verify-true
}
