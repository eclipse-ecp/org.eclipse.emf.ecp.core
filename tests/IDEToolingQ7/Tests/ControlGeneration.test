Content-Type: text/ecl
Entry-Name: .content

get-view "Package Explorer" | get-tree | select "com.eclipsesource.makeithappen.model/model/task.ecore" 
    | get-menu "EMF Forms/Create View Model" | click
with [get-window -class WizardDialog] {
    get-tree | select "task/User"
    get-button "Fill view model with default layout" | uncheck
    get-button "Next >" | click
    get-button Finish | click
}
get-editor "User.view" | get-tree | get-item View | get-property childCount | equals 0 | verify-true
get-editor "User.view" | get-tree | select View | get-menu "Generate Controls" | click
with [get-window -class SelectAttributesDialog] {
    with [get-table] {
        get-item "firstName : EString" | check
        get-item "lastName : EString" | check
        get-item "gender : Gender" | check
    }
    get-button OK | click
}
with [get-editor "User.view" | get-tree] {
    get-item View | get-property childCount | equals 3 | verify-true
    get-item "View/Control gender" | get-property caption | equals "Control gender" | verify-true
    get-item "View/Control firstName" | get-property caption | equals "Control firstName" | verify-true
    get-item "View/Control lastName" | get-property caption | equals "Control lastName" | verify-true
}
with [get-editor "User.view" | get-tree] {
    select View | get-menu VerticalLayout | click
    select "View/VerticalLayout" | get-menu "Generate Controls" | click
}
with [get-window -class SelectAttributesDialog] {
    with [get-table] {
        get-item "active : EBoolean" | check
        get-item "timeOfRegistration : EDate" | check
    }
    get-button OK | click
}
with [get-editor "User.view" | get-tree] {
    get-item "View/VerticalLayout" | get-property childCount | equals 2 | verify-true
    get-item "View/VerticalLayout/Control timeOfRegistration" | get-property "getParentItem().getText()" 
        | equals VerticalLayout | verify-true
    get-item "View/VerticalLayout/Control active" | get-property "getParentItem().getText()" | equals VerticalLayout 
        | verify-true
}
get-editor "User.view" | get-tree | select View | get-menu "Generate Controls" | click
get-window -class SelectAttributesDialog | get-button "Show only unreferenced Attributes?" | check
with [get-window -class SelectAttributesDialog | get-table] {
    get-property itemCount | equals 6 | verify-true
    get-item "weight : EDouble" | get-property caption | equals "weight : EDouble" | verify-true
}
get-window -class SelectAttributesDialog | get-table | select "heigth : EInt"
with [get-window -class SelectAttributesDialog | get-table] {
    get-item "heigth : EInt" | get-property caption | equals "heigth : EInt" | verify-true
    get-item "nationality : Nationality" | get-property caption | equals "nationality : Nationality" | verify-true
}
with [get-window -class SelectAttributesDialog | get-table] {
    get-item "dateOfBirth : DateOfBirth" | get-property caption | equals "dateOfBirth : DateOfBirth" | verify-true
    get-item "eMails : EString" | get-property caption | equals "eMails : EString" | verify-true
    get-item "tasks : Task" | get-property caption | equals "tasks : Task" | verify-true
}
get-window -class SelectAttributesDialog | get-button OK | click
