Entry-Name: .content

get-view "Project Explorer" | get-tree | select "com.eclipsesource.makeithappen.model.viewmodel/viewmodels/User.view" 
    | double-click
with [get-editor "User.view"] {
    with [get-tree] {
        select View | get-menu Control | click
        select "View/Control"
    }
    get-button "Create and link new Domain Model Reference" | click
}
with [get-window "New Reference Element"] {
    get-tree | select "model/FeaturePathDomainModelReference"
    get-button "Next >" | click
    get-button "Link Domain Model EFeature" | click
    with [get-window "Select a EStructuralFeature"] {
        get-tree | select "gender : Gender"
        get-button OK | click
    }
    get-button Finish | click
}
with [get-editor "User.view"] {
    with [get-tree] {
        select View | get-menu "Stack Layout" | click
        select "View/Stack Layout" | get-menu "Feature Path Domain Model Reference" | click
        select "View/Stack Layout/Feature Path Domain Model Reference"
    }
    get-button "Link Domain Model EFeature" | click
}
with [get-window "Select a EStructuralFeature"] {
    get-tree | select "gender : Gender"
    get-button OK | click
}
with [get-editor "User.view"] {
    with [get-tree] {
        select "View/Stack Layout" | get-menu "Stack Item" | click
        select "View/Stack Layout" | get-menu "Stack Item" | click
        select "View/Stack Layout/Stack Item"
    }
    get-button "Select Object" | click
}
get-window "Select a value" | get-button OK | click
with [get-editor "User.view"] {
    get-tree | select [get-item "View/Stack Layout" | get-item "Stack Item" -index 1]
    get-button "Select Object" | click
}
with [get-window "Select a value"] {
    get-table | select Female
    get-button OK | click
}
with [get-editor "User.view"] {
    with [get-tree] {
        select "View/Stack Layout/Stack Item" | get-menu Control | click
        select [get-item "View/Stack Layout" | get-item "Stack Item" -index 1] | get-menu Control | click
        select "View/Stack Layout/Stack Item/Control"
    }
    get-button "Create and link new Domain Model Reference" | click
}
with [get-window "New Reference Element"] {
    get-tree | select "model/FeaturePathDomainModelReference"
    get-button "Next >" | click
    get-button "Link Domain Model EFeature" | click
    with [get-window "Select a EStructuralFeature"] {
        get-tree | select "heigth : EInt"
        get-button OK | click
    }
    get-button Finish | click
}
with [get-editor "User.view"] {
    get-tree | select [get-item "View/Stack Layout" | get-item "Stack Item" -index 1 | get-item Control]
    get-button "Create and link new Domain Model Reference" | click
}
with [get-window "New Reference Element"] {
    get-tree | select "model/FeaturePathDomainModelReference"
    get-button "Next >" | click
    get-button "Link Domain Model EFeature" | click
    with [get-window "Select a EStructuralFeature"] {
        get-tree | select "weight : EDouble"
        get-button OK | click
    }
    get-button Finish | click
}
get-button "Save (M1+S)" | click
get-editor "User.view" | get-button "Open Preview" | click
get-view "EMF Forms Preview" | get-button "Refresh Preview View" | click
with [get-view "EMF Forms Preview"] {
    get-combo -after [get-label Gender] | get-property selection | equals Male | verify-true
    get-control Any -after [get-label "View Editor Preview"] -index 6 
        | get-property "getChildren().Control[0].getChildren().Control[0].getChildren().length" | equals 4 | verify-true
    get-label Heigth | get-property caption | equals Heigth | verify-true
}
get-view "EMF Forms Preview" | get-combo -after [get-label Gender] | select Female
with [get-view "EMF Forms Preview"] {
    get-combo -after [get-label Gender] | get-property selection | equals Female | verify-true
    get-control Any -after [get-label "View Editor Preview"] -index 6 
        | get-property "getChildren().Control[0].getChildren().Control[0].getChildren().length" | equals 4 | verify-true
    get-label Weight | get-property caption | equals Weight | verify-true
}
