Content-Type: text/ecl
Entry-Name: .content

get-view "Project Explorer" | get-tree | select "com.eclipsesource.makeithappen.model.viewmodel/viewmodels/User.view" 
    | double-click
get-editor "User.view" | close
get-view "Project Explorer" | get-tree 
    | select "com.eclipsesource.makeithappen.model.viewmodel/viewmodels/UserGroup.view" | double-click
with [get-editor "UserGroup.view"] {
    with [get-tree] {
        select View | get-menu TableControl | click
        select "View/TableControl"
    }
    get-button "Create and link new Domain Model Reference" | click
}
with [get-window "New Reference Element"] {
    get-button "Link Domain Model EFeature" | click
    with [get-window "Select a EStructuralFeature"] {
        get-tree | select "users : User"
        get-button OK | click
    }
    get-button Finish | click
}
with [get-editor "UserGroup.view"] {
    get-tree | select "View/TableControl" | get-menu "Generate Columns" | click
    get-combo -after [get-label "Detail Editing*"] | select WithPanel
}
get-editor "UserGroup.view" | get-label "Detail View" | get-property caption | equals "Detail View" | verify-true
get-eclipse-window | key-type "M1+s"
get-editor "UserGroup.view" | get-button "Open Preview" | click
get-view "EMF Forms Preview" | get-button "Refresh Preview View" | click
get-view "EMF Forms Preview" | get-control Any -after [get-label Users] -index 10 
    | get-property "getParent().getParent().getStyle()" | equals "[BORDER, LEFT_TO_RIGHT]" | verify-true
with [get-view "EMF Forms Preview"] {
    get-button "Add an instance of" | click
    with [get-table] {
        select Male -column Gender | activate-cell-edit -column 7
        get-editbox | click-text 1 2
        apply-cell-edit -deactivate
    }
}
get-view "EMF Forms Preview" | get-label "First Name" | get-property caption | equals "First Name" | verify-true
get-editor "UserGroup.view" | get-button "Create and link new Detail View" | click
get-editor "UserGroup.view" | get-label View | get-property caption | equals View | verify-true
get-editor "UserGroup.view" | get-tree | select "View/TableControl/View" | get-menu "Generate Controls" | click
with [get-window -class SelectAttributesDialog] {
    get-tree | select User
    get-button "Next >" | click
    get-table | get-item "lastName : EString" | check
    get-button Finish | click
}
get-editor "UserGroup.view" | get-tree | key-type "M1+s"
with [get-view "EMF Forms Preview"] {
    get-button "Refresh Preview View" | click
    with [get-table] {
        select Male -column Gender | activate-cell-edit -column 1
        get-editbox | click-text
        apply-cell-edit -deactivate
    }
}
with [get-view "EMF Forms Preview"] {
    get-label "Last Name*" | get-property caption | equals "Last Name*" | verify-true
    get-editbox -after [get-label "Last Name*"] | get-property backgroundColor | equals "#FF0000" | verify-true
    get-control Any -after [get-label Users] -index 12 | get-property "getChildren().length" | equals 3 | verify-true
}
with [get-editor "UserGroup.view"] {
    get-tree | select "View/TableControl"
    get-button "Delete Reference" -after [get-label View] | click
}
get-editor "UserGroup.view" | get-label -after [get-label "Detail View"] -index 2 | get-property caption | equals "" 
    | verify-true
with [get-view "EMF Forms Preview"] {
    with [get-table] {
        select Male -column Gender | activate-cell-edit -column 2
        get-editbox | click-text
    }
    get-button "Refresh Preview View" | click
    with [get-table] {
        cancel-cell-edit
        deactivate-cell-edit
    }
    with [get-table] {
        select Male -column Gender | activate-cell-edit -column 1
        get-editbox | click-text
        apply-cell-edit -deactivate
    }
}
get-view "EMF Forms Preview" | get-control Any -after [get-label Users] -index 12 | get-property "getChildren().length" 
    | equals 30 | verify-true
